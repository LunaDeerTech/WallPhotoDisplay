{
  "name": "wall-photo-display",
  "version": "1.0.0",
  "description": "多用户照片墙系统 - Multi-user Photo Wall PWA",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "tsx watch server/index.ts",
    "dev:client": "vite",
    "build": "vue-tsc && vite build",
    "build:server": "tsc -p tsconfig.server.json",
    "start": "node dist/server/index.js",
    "init-db": "tsx server/scripts/initDatabase.ts",
    "typecheck": "vue-tsc --noEmit && tsc -p tsconfig.server.json --noEmit",
    "preview": "vite preview",
    "generate-icons": "node scripts/generateIcons.js"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.2.2",
    "connect-history-api-fallback": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "sharp": "^0.33.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/connect-history-api-fallback": "^1.5.4",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.10.1",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/tsconfig": "^0.8.1",
    "axios": "^1.6.2",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "pinia": "^2.1.7",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^5.0.0",
    "vite-plugin-pwa": "^0.17.0",
    "vue": "^3.3.8",
    "vue-tsc": "^3.1.5"
  },
  "keywords": [
    "photo-wall",
    "vue3",
    "express",
    "pwa",
    "sqlite"
  ],
  "author": "",
  "license": "MIT"
}
